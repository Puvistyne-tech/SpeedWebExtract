const getDataFED = async url => {
  const [xml = null, date = ""] = await Object(_utils__WEBPACK_IMPORTED_MODULE_0__["getXML"])(url);
  const fedJs = await Object(_utils__WEBPACK_IMPORTED_MODULE_0__["convertXml2js"])(xml); // console.log(fedJs)

  const messageGroup = fedJs.MessageGroup;
  const name = messageGroup.Header.Name;
  const prepared = " (" + messageGroup.Header.Prepared + ")";
  const series = messageGroup.DataSet.Series;
  const columnDefs = createColDefs(series);
  const rowData = createRowData(series, columnDefs);
  return [name, prepared, date, columnDefs, rowData];
};

