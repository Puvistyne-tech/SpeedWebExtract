async function getData(nom, url) {
  let data = ""; // console.log(url)

  switch (nom) {
    // case "Webstat":
    //   data = await webstat.getDataWebstat(url);
    //   break;
    case "l'INSEE":
      data = await insee.getDataINSEE(url);
      break;

    case "Bank of England":
      data = await boe.getDataBoE(url);
      break;

    case "Eurostat":
      data = await eurostat.getDataEurostat(url);
      break;

    case "SDW":
      data = await sdw.getDataSDW(url);
      break;

    case "FED":
      data = await fed.getDataFED(url);
      break;

    case "All":
      data = await bdf.getDataBdF();
      break;

    default:
      break;
  } // console.log(data)


  return data;
} // /**
//  *
//  * @param {string} url
//  */
