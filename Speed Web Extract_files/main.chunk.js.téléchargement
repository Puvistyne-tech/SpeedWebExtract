      dim6 = "CIL";
      desc6 = "Commercial and industrial loans";
      dim10 = "2240";
      desc10 = "NFCs";
      break;

    case 1026:
      dim6 = "A22";
      desc6 = "Real estates loans";
      dim10 = "0000";
      desc10 = "All";
      break;

    case 1221:
      dim6 = "A22";
      desc6 = "Residential real estate loans";
      dim10 = "2250";
      desc10 = "Households";
