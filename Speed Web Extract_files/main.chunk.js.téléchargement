const JefNode = __webpack_require__(/*! json-easy-filter */ "./node_modules/json-easy-filter/index.js").JefNode;

const outputType = data => {
  return new JefNode(data).filter(function (node) {
    if (node.has("CAT_NAME") && node.value.CAT_NAME === "OUTPUT IN") {
      return node.value.CAT_VAL;
    }
  });
};

const formatterBOE = data => {
  if (!data.Envelope.Cube[0]) {
    data.Envelope.Cube = [data.Envelope.Cube];
    return data;
  } else {
    return data;
  }
};

const getDataName = cube => {
  let name = [];

  try {
    for (let i = 0; i < (cube.length < 10 ? cube.length : 10); i++) {
      const element = cube[i];
      name.push(element.$.SCODE);
    } // cube.map((element) => {
    //   //   console.log(element.$.SCODE)
    //   name.push(element.$.SCODE);
    // });


    name.join(",");
    return name + (cube.length > 10 ? " ..." : "");
  } catch {}

  name.push(cube.$.SCODE);
  return name;
}; // const rowPercentage = (cube) => {
//   let children = [];
