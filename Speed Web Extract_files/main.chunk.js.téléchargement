/**
 *
 * @param {application/xml} xml
 * @param {json:string} option
 */

const convertXml2js = xml => {
  let option = {
    explicitArray: false,
    tagNameProcessors: [stripPrefix],
    attrValueProcessors: [parseNumbers]
  };
  return new Promise((resolve, reject) => {
    xml2js.parseString(xml, option, (err, jsonObj) => {
      if (err) {
        return reject(err);
      } //convertion de JS objet à JSON string
      // resolve(JSON.stringify(jsonObj));


      resolve(jsonObj);
    });
  });
};
const currencyFormat = params => {
  if (params.value === undefined || params.value === null || params.value === ":" || params.value === "") {
    return ":";
  } else if (params.value === -999999.9999999) {
    return 0.0000001;
  } else {
    return currencyFormatter.format(params.value, {
      code: "",
      thousand: " ",
      decimal: ","
    });
  }
}; /////DIMENSIONS 1-12
