const bdf = {
  nom: "All",
  img: "banque-de-france.png",
  data: []
};

function App() {
  const [source, setSource] = Object(react__WEBPACK_IMPORTED_MODULE_0__["useState"])(dataSource);

  const updateSource = (id, col, row, info) => {
    if (source && id && col && row && info) {
      let element = source;
      setSource([]);
      element.forEach(oneElement => {
        oneElement.data.forEach(oneData => {
          if (oneData.$ === id) {
            oneData.donnees.columnDefs = col;
            oneData.donnees.rowData = row;
            oneData.date = info.date;
            oneData.prepared = info.prepared;
            oneData.nom = info.dataName.toString();
          }
        });
      });
      setSource([]);
      setSource(element);
      updateBdfData();
    } else {
      console.log("updateSource Erreur ....");
    }
  };

  const [bdfData, setBdfData] = Object(react__WEBPACK_IMPORTED_MODULE_0__["useState"])(bdf);

  const updateBdfData = () => {
    // if (source) {
    let temp = bdfData; // setBdfData([]);

    let res = []; // console.log(source)

    source.forEach(oneSource => {
      let col = [];
      let row = [];
      oneSource.data.forEach(oneData => {
        // console.log( oneData.donnees.columnDefs)
        // if (
        //   oneData.donnees.columnDefs.length > 0 &&
        //   oneData.donnees.rowData.length > 0
        // ) {
        col = Object(_components_utils_source__WEBPACK_IMPORTED_MODULE_14__["mergeColDefs"])(oneData.donnees.columnDefs, col);
        row = Object(_components_utils_source__WEBPACK_IMPORTED_MODULE_14__["mergeRowDatas"])(oneData.donnees.rowData, row); // }
      }); // console.log(col)

      if (col && row && col.length && row.length) {
        res.push({
          $: Object(uuid__WEBPACK_IMPORTED_MODULE_3__["v4"])(),
          nom: oneSource.nom,
          date: new Date(),
          donnees: {
            columnDefs: col,
            rowData: row
          }
        });
      }
    });
    temp.data = res; // console.log(temp)

    setBdfData(temp); // } else {
    //   console.log("update Bdf Data erreur....");
    // }
  };

  Object(react__WEBPACK_IMPORTED_MODULE_0__["useEffect"])(() => {
    console.log("bdfData update ....");
    updateBdfData();
  });
  const [columnDefs, setColumnDefs] = Object(react__WEBPACK_IMPORTED_MODULE_0__["useState"])([]);
  const [rowData, setRowData] = Object(react__WEBPACK_IMPORTED_MODULE_0__["useState"])([]);
  const [showGrid, setShowGrid] = Object(react__WEBPACK_IMPORTED_MODULE_0__["useState"])(false);
  const [activeGridName, setActiveGridName] = Object(react__WEBPACK_IMPORTED_MODULE_0__["useState"])("");

  const lanceGridBdf = element => {
    // console.log(element);
    if (element.$) {
      console.log(bdf);
      bdf.data.forEach(oneData => {
        if (oneData.$ === element.$) {
          setColumnDefs(oneData.donnees.columnDefs);
          setRowData(oneData.donnees.rowData);
          setActiveGridName("All/" + oneData.nom);
        }
      });
      setShowGrid(true);
    } else if (element.nom && element.nom === "All") {
      let col = [];
      let row = []; // console.log(bdf.data)

      bdf.data.forEach(oneData => {
        // console.log(col)
        col = Object(_components_utils_source__WEBPACK_IMPORTED_MODULE_14__["mergeColDefs"])(oneData.donnees.columnDefs, col);
        row = Object(_components_utils_source__WEBPACK_IMPORTED_MODULE_14__["mergeRowDatas"])(oneData.donnees.rowData, row);
      }); // console.log(col)

      setColumnDefs(col);
      setRowData(row);
      setActiveGridName("All");
      setShowGrid(true);
    }
  };

  const lanceGrid = element => {
    if (element.$) {
      source.forEach(oneSource => {
        oneSource.data.forEach(oneData => {
          if (oneData.$ === element.$) {
            setColumnDefs(oneData.donnees.columnDefs);
            setRowData(oneData.donnees.rowData);
            setActiveGridName(oneSource.nom);
          }
        });
      });
      setShowGrid(true);
    } else if (element.nom) {
      source.forEach(oneSource => {
        if (oneSource.nom === element.nom) {
          // console.log(oneSource);
          let col = [];
          let row = [];
          oneSource.data.forEach(oneData => {
            // console.log(col)
            col = Object(_components_utils_source__WEBPACK_IMPORTED_MODULE_14__["mergeColDefs"])(oneData.donnees.columnDefs, col);
            row = Object(_components_utils_source__WEBPACK_IMPORTED_MODULE_14__["mergeRowDatas"])(oneData.donnees.rowData, row);
          });
          setColumnDefs(col);
          setRowData(row);
          setActiveGridName(oneSource.nom);
        }
      });
      setShowGrid(true);
    }
  };

  const closeGrid = () => {
    setColumnDefs([]);
    setRowData([]);
    setShowGrid(false);
    setActiveGridName();
  };

  const [isOpen, setIsOpen] = Object(react__WEBPACK_IMPORTED_MODULE_0__["useState"])(false);

  const handleToggle = () => {
    setIsOpen(!isOpen);
  };

  const openProject = res => {
    setSource(res.source);
    setBdfData(res.bdf);
    updateBdfData();
  };

  const handleNewGridClick = () => {
    setColumnDefs([]);
    setRowData([]);
    setShowGrid(true);
    setActiveGridName("new");
  };

  const [showSourceOption, setShowSourceOption] = Object(react__WEBPACK_IMPORTED_MODULE_0__["useState"])(false);

  const handleSourceClick = () => {
    setShowSourceOption(!showSourceOption);
  };

  const addNewUrl = res => {
    setSource(res);
  };

  const [showModal, setShowModal] = Object(react__WEBPACK_IMPORTED_MODULE_0__["useState"])(false);
  const [mgs, setMgs] = Object(react__WEBPACK_IMPORTED_MODULE_0__["useState"])("");
  return (
    /*#__PURE__*/
    // <ErrorHandler>
    react__WEBPACK_IMPORTED_MODULE_0___default.a.createElement(react_router_dom__WEBPACK_IMPORTED_MODULE_2__["BrowserRouter"], {
