const getDim4 = element => {
  let dim = "_";
  let desc = "_";

  switch (element) {
    case "SA":
      dim = "A";
      desc = "Seasonally adjusted";
      break;

    case "NSA":
      dim = "N";
      desc = "Non Seasonally adjusted";
      break;

    default:
      dim = "N";
      desc = "Non Seasonally adjusted";
      break;
  }

  return [dim, desc];
};

