const createColDefs = series => {
  let columnDefs = [_utils__WEBPACK_IMPORTED_MODULE_0__["columnDefsSDMXHeader"], _utils__WEBPACK_IMPORTED_MODULE_0__["columnDefsSDMXExplication"], createDateCols(series)];
