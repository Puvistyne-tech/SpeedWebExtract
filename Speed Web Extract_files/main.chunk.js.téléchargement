/***/ "./src/components/utils/date.js":
/*!**************************************!*\
  !*** ./src/components/utils/date.js ***!
  \**************************************/
/*! exports provided: createDateRange, getYYYYMM */
/***/ (function(module, __webpack_exports__, __webpack_require__) {

"use strict";
__webpack_require__.r(__webpack_exports__);
/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, "createDateRange", function() { return createDateRange; });
/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, "getYYYYMM", function() { return getYYYYMM; });
/**
 * startDate- date de début de la colonne
 * endDate- date de fin de la colonne
 */
const createDateRange = (startDate, endDate) => {
  let dateCol = [];

  if (!startDate || startDate.length === 0) {
    startDate = new Date("2010-01-01");
  } else {
    startDate = new Date(startDate);
  }

  if (!endDate || endDate.length === 0) {
    endDate = new Date();
  } else {
    endDate = new Date(endDate);
  }

  let loop = new Date(startDate);

  while (loop <= endDate) {
    dateCol.push(getYYYYMM(loop));
    let newDate = loop.setMonth(loop.getMonth() + 1);
    loop = new Date(newDate);
  }

  return dateCol;
};
function getYYYYMM(date) {
  let currentDate = "";
  let today = new Date(date);
  let dd = String(today.getDay()).padStart(2, "0");
  let mm = String(today.getMonth() + 1).padStart(2, "0"); //0 est Janvier

  let yyyy = today.getFullYear(); //   currentDate = yyyy + "-" + mm + "-" + dd;

  currentDate = yyyy + "-" + mm;
  return currentDate;
} //exemple
// let datecol=createDateCol();
// console.log(datecol)
// export createDateCol
// module.exports = { createDateRange, getYYYYMM };

/***/ }),

/***/ "./src/components/utils/dim/dim1.js":
/*!******************************************!*\
  !*** ./src/components/utils/dim/dim1.js ***!
  \******************************************/
/*! exports provided: getDim1 */
/***/ (function(module, __webpack_exports__, __webpack_require__) {

"use strict";
__webpack_require__.r(__webpack_exports__);
/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, "getDim1", function() { return getDim1; });
const getDim1 = element => {
  let dim = "_";
  let desc = "_";

  switch (element) {
    case "FED":
      dim = "FED";
      desc = "Federal Reserve";
      break;

    case "BOE":
      dim = "BOE";
      desc = "Bank of England";
      break;

    case "SDW":
      dim = "SDW";
      desc = "Banque Centrale Européenne";
      break;

    case "INN":
      dim = "INN";
      desc = "l'INSEE";
      break;

    case "WEB":
      dim = "WEB";
      desc = "Webstat";
      break;

    case "EST":
      dim = "EST";
      desc = "Eurostat";
      break;

    default:
      dim = "___";
      desc = "_____________";
      break;
  }

  return [dim, desc];
};

